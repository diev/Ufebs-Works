<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>ReturnSWIFT</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyVersion>1.22.9.5</AssemblyVersion>
    <FileVersion>1.22.9.5</FileVersion>
	<Version>1.22.9.5</Version>
    <Company>Cibank</Company>
    <Copyright>2022 Dmitrii Evdokimov</Copyright>
    <Description>Преобразование обратных пакетов из SWIFT в ESID UFEBS, полученных из ЦБ по СПФС через корсчет в другом банке.</Description>	  
    <NeutralLanguage>ru-RU</NeutralLanguage>
    <PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
  </PropertyGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="if $(ConfigurationName) == Release call ..\PostBuild.bat" />
  </Target>

  <ItemGroup>
    <ProjectReference Include="..\Corr-Lib\Corr-Lib.csproj" />
  </ItemGroup>

</Project>
