<Project Sdk="Microsoft.NET.Sdk">

  <PropertyGroup>
    <OutputType>Exe</OutputType>
    <TargetFramework>net6.0</TargetFramework>
    <RootNamespace>ReturnReplace</RootNamespace>
    <ImplicitUsings>enable</ImplicitUsings>
    <Nullable>enable</Nullable>
    <AssemblyVersion>1.22.10.28</AssemblyVersion>
    <FileVersion>1.22.10.28</FileVersion>
	<Version>1.22.10.28</Version>
	<PackageLicenseExpression>Apache-2.0</PackageLicenseExpression>
    <Copyright>2022 Dmitrii Evdokimov</Copyright>
    <Description>Преобразование обратных пакетов ESID, полученных из ЦБ через корсчет в другом банке.</Description>
    <Company>Cibank</Company>
    <NeutralLanguage>ru-RU</NeutralLanguage>
  </PropertyGroup>

  <Target Name="PostBuild" AfterTargets="PostBuildEvent">
    <Exec Command="if $(ConfigurationName) == Release call ..\PostBuild.bat" />
  </Target>

</Project>
